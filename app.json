{
  "expo": {
    "name": "SJPE",
    "description": "Aplicacion informativa para sociedad juvenil",
    "slug": "sjpe_app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo_sjpe.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roelmdz.sjpe"
    },
    "android": {
      "package": "com.roelmdz.sjpe"
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B"
    }
  }
}
